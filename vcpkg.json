{
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
    "name": "actionsrepotest",
    "version": "0.1.0-dev",
    "dependencies": [
        {
            "name": "libarchive",
            "default-features": true,
            "features": [ "bzip2", "lz4", "lzma", "zstd" ]
        },
        "boost-core",
        "boost-algorithm",
        "boost-filesystem",
        {
            "name": "boost-interprocess",
            "platform": "windows"
        },
        {
            "name": "sdl2",
            "features": [ "vulkan" ]
        },
        "tinyxml2",
        "glew",
        "glm",
        "jsoncpp",
        "openexr",
        "vulkan",
        "vulkan-headers"
    ]
}

